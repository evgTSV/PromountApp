            <stringProp name="script">import java.util.UUID
import groovy.json.JsonOutput

def generateUUID() {
    return UUID.randomUUID()
}

def generateAdvertiserObject(index) {
    return [
        &quot;advertiser_id&quot;: generateUUID(),
        &quot;name&quot;: &quot;string$index&quot;
    ]
}

def advertisers = []

for (int i = 0; i &lt; 1; i++) {
	advertisers.add(generateAdvertiserObject(52))
}

def json = JsonOutput.toJson(advertisers)

vars.put(&quot;generated_json&quot;, json)
