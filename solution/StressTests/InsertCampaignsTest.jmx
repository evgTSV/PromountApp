            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import java.util.UUID
import java.util.Random
import groovy.json.JsonOutput

def random = new Random()
def impress = random.nextInt(20) + 1

def generateCampaignObject(index, random, impress) {
    return [
        &quot;impressions_limit&quot;: impress,
        &quot;clicks_limit&quot;: random.nextInt(impress) + 1,
        &quot;cost_per_impression&quot;: random.nextInt(20) + 1,
        &quot;cost_per_click&quot;: random.nextInt(20) + 1,
        &quot;ad_title&quot;: &quot;string$index&quot;,
        &quot;ad_text&quot;: &quot;string$index&quot;,
        &quot;start_date&quot;: 0,
        &quot;end_date&quot;: 100,
        &quot;targeting&quot;: [:]
    ]
}

def campaigns = generateCampaignObject(52, random, impress)

def json = JsonOutput.toJson(campaigns)

vars.put(&quot;generated_json&quot;, json)

// Выводим результат для отладки
log.info(&quot;Generated campaigns JSON: ${json}&quot;)
